

<h1>명함</h1>
<table class="table table-sm table-bordered" style="width : 800px">
  <%
    var i = 0;
    
    while(i < results.length){
  %>
    <tr><td class="td td-smd" ><%=results[i].loginid%></td>
        <td><%=results[i].name%></td>
        <td><%=results[i].company%></td>
        <td><%=results[i].company_position%></td>
        <td><%=results[i].email%></td>
        <td><%=results[i].tel%></td>
        <td><%=results[i].birth%></td>
      </tr>
  <%
    i++;
    }
  %>
</table>